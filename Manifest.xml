<?xml version="1.0" encoding="utf-8"?>
<manifest>
	<application>
	<name>webim</name>
	<alias>webim</alias>
	<version>1.0</version>
	<pw-version>9.0</pw-version>
	<description>WebIM插件，社区站内聊天。</description>
	<logo>images/logo.jpg</logo>
	<type>app</type>
	<charset>UTF-8</charset>
	<website>http://www.webim20.cn</website>
	<author-name>杭州巨鼎信息技术有限公司</author-name>
	<author-email>webim20@gmail.com</author-email>
	<author-icon>your icon</author-icon>
	</application>
	<res>res</res>
	<installation-service>
	</installation-service>
	<inject-services>
	<s_admin_menu>
	<app_webim>
	<class>EXT:webim.service.srv.App_Webim_ConfigDo</class>
	<loadway>load</loadway>
	<method>getAdminMenu</method>
	<description>webimadmin menu</description>
	</app_webim>
	</s_admin_menu>
	<s_footer>
	<app_webim>
	<class>EXT:webim.service.srv.App_Webim_FooterDo</class>
	<loadway>load</loadway>
	<method>createHtml</method>
	<description>inject webim javascipt</description>
	</app_webim>
	</s_footer>
	</inject-services>
	</manifest>
